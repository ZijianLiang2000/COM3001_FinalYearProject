        innerHtml = innerHtml + '<br><br><form class="button_to" method="post" action="/location_in_cluster?&rest_cat='+ gon.rest_cat +'&lad_name='+ gon.lad_name +'&lsoa_code='+ gon.lsoa11_code + '&cluster='+ sorted[i]  + '&place_id_arr='+ info_arr + '&loc_strategy='+ gon.loc_strategy +'">';
