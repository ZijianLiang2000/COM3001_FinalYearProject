                // Google map frame
                '<div class="holder"><iframe id="map_frame"' +
                ' width="450"' +
                ' height="705"' +
                ' style="border:0"' +
                ' loading="lazy"' +
                ' allowfullscreen' +
                src +
                '</iframe>' +
                // Card to display information about district
